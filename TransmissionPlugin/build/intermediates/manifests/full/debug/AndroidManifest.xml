<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atooma.plugin.transmission"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <service android:name="com.atooma.sdk.RegisterService" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/plugin_icon_normal"
        android:label="@string/app_name" >
        <service
            android:name="com.atooma.AtoomaPluginService"
            android:process=":remote" />

        <receiver android:name="com.atooma.plugin.transmission.TRANSMISSIONATOOMAMODULEReceiver" >
            <intent-filter>
                <action android:name="com.atooma.plugin.REGISTRATION" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.atooma.plugin.transmission.TRANSMISSIONATOOMAMODULERegister"
            android:process=":remote" />
    </application>

</manifest>