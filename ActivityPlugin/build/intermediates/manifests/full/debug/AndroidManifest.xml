<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atooma.plugin.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <service android:name="com.atooma.sdk.RegisterService" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name" >
        <activity
            android:name="com.atooma.plugin.activity.MainActivity"
            android:exported="true" />

        <service
            android:name="com.atooma.AtoomaService"
            android:process=":remote" />

        <receiver android:name="com.atooma.plugin.activity.ACTIVITYReceiver" >
            <intent-filter>
                <action android:name="com.atooma.plugin.REGISTRATION" />
            </intent-filter>
        </receiver>

        <service android:name="com.atooma.plugin.activity.ACTIVITYRegister" />
    </application>

</manifest>